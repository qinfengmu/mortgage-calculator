<view><view class="calculator-form-item"><view class="form-row"><label class="form-label" for>商业贷款金额（万元）</label><input class="form-input" type="number" placeholder="请输入商业贷款金额" data-event-opts="{{[['input',[['__set_model',['','loanAmount','$event',[]]]]]]}}" value="{{loanAmount}}" bindinput="__e"/></view><view class="form-row slt-row"><label class="form-label" for>商业贷款利率</label><view class="slt-text"><picker value="{{rateIndex}}" range-key="text" range="{{rateArray}}" data-event-opts="{{[['change',[['bindRatePickerChange',['$event']]]]]}}" bindchange="__e"><view class="uni-input">{{rateArray[rateIndex].text}}</view></picker></view></view><view class="form-row"><label class="form-label" for>公积金贷款金额（万元）</label><input class="form-input" type="number" placeholder="请输入公积金贷款金额" data-event-opts="{{[['input',[['__set_model',['','acc_loanAmount','$event',[]]]]]]}}" value="{{acc_loanAmount}}" bindinput="__e"/></view><view class="form-row slt-row"><label class="form-label" for>公积金贷款利率</label><view class="slt-text"><picker value="{{acc_rateIndex}}" range-key="text" range="{{acc_rateArray}}" data-event-opts="{{[['change',[['bindAccRatePickerChange',['$event']]]]]}}" bindchange="__e"><view class="uni-input">{{acc_rateArray[acc_rateIndex].text}}</view></picker></view></view><view class="form-row slt-row"><label class="form-label" for>贷款期限</label><view class="slt-text"><picker value="{{phaseIndex}}" range-key="text" range="{{phaseArray}}" data-event-opts="{{[['change',[['bindPhasePickerChange',['$event']]]]]}}" bindchange="__e"><view class="uni-input">{{phaseArray[phaseIndex].text}}</view></picker></view></view><view class="form-row slt-row"><label class="form-label" for>还款方式</label><view class="slt-text"><picker value="{{paymentTypeIndex}}" range-key="text" range="{{paymentTypeArray}}" data-event-opts="{{[['change',[['bindTypePickerChange',['$event']]]]]}}" bindchange="__e"><view class="uni-input">{{paymentTypeArray[paymentTypeIndex].text}}</view></picker></view></view><view class="form-bottom"><button class="form-btn" type="primary" data-event-opts="{{[['tap',[['calc']]]]}}" bindtap="__e">开始计算</button></view></view></view>